bool containsNearbyDuplicate(int* nums, int numsSize, int k) 
{
    for(int i=0;i<numsSize;i++)
    {
        for(int j=i-1;j>-1;j--)
        {
            if(nums[i]>nums[j])
                break;
            else if(nums[i]==nums[j]&&(i-j)<=k)
                return true;
        }
    }
    return false;
}
